<h2>Sign up</h2>
<% if @user && @user.errors.any? %>
  <% @user.errors.full_messages.each do |error| %>
  <ul>
    <li>
      <%= error %>
    </li>
  </ul>
  <% end %>
<% end %>

<%= form_for(:user, url: users_path) do |f| %>
  <%= render partial: "sessions/signup_form", locals: {f: f} %>
<% end %>

